<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="multibrowzer" parallel="tests" thread-count="5">
	<test name="chrometest">
		<parameter name="browzer" value="chrome"></parameter>
		<classes>
			<class name="test_class.Homepage_Test"></class>
		</classes>
	</test>
	<test name="edgetest">
		<parameter name="browzer" value="edge"></parameter>
		<classes>
			<class name="test_class.Homepage_Test"></class>
		</classes>
	</test>
	<test name="firefoxtest">
		<parameter name="browzer" value="firefox"></parameter>
		<classes>
			<class name="test_class.Homepage_Test"></class>
		</classes>
	</test>
</suite>